{
  "name": "darkghosthunter/fluid",
  "description": "Fluid utility class based on Laravel's Fluent class",
  "minimum-stability": "dev",
  "license": "MIT",
  "authors": [
    {
      "name": "Italo Baeza Cabrera",
      "email": "darkghosthunter@gmail.com"
    }
  ],
  "require": {
    "php" : ">=7.4",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.3"
  },
  "autoload": {
    "psr-4": {
      "DarkGhostHunter\\Fluid\\": "src"
    }
  },
  "autoload-dev": {
